void setup() {
  // Initialize the backlight pin
  pinMode(TFT_BL, OUTPUT);
  digitalWrite(TFT_BL, HIGH); // Turn on the backlight

  // Initialize the display
  tft.initR(INITR_BLACKTAB); // Specific initialization for the ST7735 screen
  tft.setRotation(2);
  tft.fillScreen(0x0000); // Clear the screen with a black background
}

void loop() {
  for (int y = 0; y < 32; y++) {
    for (int x = 0; x < 32; x++) {
      tft.fillRect(x * 4, y * 4, 4, 4, frame_0[y * 32 + x]);
    }
  }

  delay(100);

  for (int y = 0; y < 32; y++) {
    for (int x = 0; x < 32; x++) {
      tft.fillRect(x * 4, y * 4, 4, 4, frame_1[y * 32 + x]);
    }
  }

  delay(100);

  for (int y = 0; y < 32; y++) {
    for (int x = 0; x < 32; x++) {
      tft.fillRect(x * 4, y * 4, 4, 4, frame_2[y * 32 + x]);
    }
  }

  delay(100);

  for (int y = 0; y < 32; y++) {
    for (int x = 0; x < 32; x++) {
      tft.fillRect(x * 4, y * 4, 4, 4, frame_3[y * 32 + x]);
    }
  }

  delay(100);
}
